/**
 * Next.js insists on using the main `tsconfig.json` file so this is actually
 * the config file for Next.js
 */
{
  "extends": "@bratislava-wysimark/tsconfig/tsconfig.custom.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "~/*": ["*"]
    },
    "jsx": "react-jsx"
  }
}
