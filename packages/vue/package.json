{
  "name": "@wysimark/vue",
  "version": "3.0.20",
  "license": "MIT",
  "author": "Sunny Hirai <thesunny@gmail.com>",
  "main": ".dist/index.js",
  "types": ".dist/index.d.ts",
  "module": ".dist/index.mjs",
  "files": [
    "README.md",
    ".dist/"
  ],
  "devDependencies": {
    "@thesunny/assert-type": "^0.1.13",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/jest": "^27",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.11.18",
    "@types/prismjs": "^1.26.0",
    "@types/react": ">=17.x",
    "@types/react-dom": ">=17.x",
    "@vitejs/plugin-vue": "^4.2.1",
    "@wysimark/standalone": "workspace:*",
    "change-case": "^4.1.2",
    "concurrently": "^7.2",
    "csstype": "^3.0.2",
    "eslint": "^8.4.1",
    "eslint-config-custom": "workspace:*",
    "jest": "^27",
    "mdast-util-definitions": "^5.1.1",
    "plop": "^2",
    "prettier": "^2.8",
    "pretty-bytes": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^27",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.2",
    "tsup": "^6.7.0",
    "type-fest": "^3.2.0",
    "typescript": "^4.9.4",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "vite": "^4.5.5",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.1.0",
    "@emoji-mart/react": "^1.1.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@portive/client": "^10.0.3",
    "clsx": "^1.2.1",
    "dotenv": "^16.3",
    "emoji-mart": "^5.4.0",
    "is-hotkey": "^0.2.0",
    "just-map-values": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "nanoid": "^3.3.6",
    "prismjs": "^1.29.0",
    "react": ">=17.x",
    "react-dom": ">=17.x",
    "slate": "^0.85.0",
    "slate-history": "^0.85.0",
    "slate-react": "^0.83.2",
    "zustand": "^4.1.5"
  },
  "scripts": {
    "--build": "# build with tsup.config.js",
    "build": "rimraf .dist && vite build",
    "build:watch": "pnpm build --watch",
    "-- fix": "# fix syntax",
    "fix": "pnpm fix:prettier && pnpm fix:eslint",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm lint:prettier --write",
    "-- lint": "# lint syntax",
    "lint": "concurrently 'pnpm lint:prettier' 'pnpm lint:eslint' 'pnpm lint:tsc'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint:tsc": "tsc --build ./ --force",
    "-- next": "# NextJs",
    "open": "open -a 'google chrome' http://localhost:3732",
    "start:local": "DOTENV=.env/local.env next dev -p 3732",
    "-- plop": "# Plop: specify ts-node https://github.com/plopjs/plop/issues/192",
    "plop": "TS_NODE_PROJECT=tsconfig.ts-node.json plop --plopfile plop/plopfile.ts",
    "-- release": "# publish to npm",
    "release": "pnpm build && npm publish",
    "--- test": "# Temporarily disabling tests as there are none yet",
    "z:test:clearcache": "jest --clearCache",
    "z:test:once": "pnpm test:clearcache && jest --config=jest.config.js",
    "z:test:watch": "jest --watch --config=jest.fast.config.js",
    "z:test:watch:types": "pnpm test:clearcache && jest --watch --config=jest.config.js",
    "-- preset": "# Run preset from ~/presets",
    "preset": "ts-node --project $HOME/presets/tsconfig.ts-node.json $HOME/presets/bin/index.ts",
    "-- end": "# end"
  }
}
